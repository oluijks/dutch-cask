    "@angular/pwa": "^0.6.8",
    "@angular/router": "^6.1.9",
    "@angular/service-worker": "^6.1.9",
    "@dc/api": "file:dist/api/dc-api-0.0.1.tgz",
    "@ngrx/effects": "^6.1.0",
